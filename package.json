{
  "name": "corners",
  "version": "0.0.13",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "esbuild example/client.tsx --outfile=example/web/client.js --servedir=example/web --serve=6144 --bundle --minify --sourcemap",
    "build:example": "esbuild example/client.tsx --outfile=example/web/client.js --bundle --minify --sourcemap",
    "predeploy": "yarn build:example",
    "deploy": "gh-pages -d example/web",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:once": "vitest run",
    "lint": "eslint \"{src,test}/**/*.ts{,x}\"",
    "lint:fix": "yarn lint -- --fix",
    "release": "yarn build && changeset publish"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@banka/eslint-config-react": "1.3.0",
    "@changesets/cli": "2.26.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@types/react": "17.0.53",
    "@types/react-dom": "17.0.19",
    "c8": "7.12.0",
    "esbuild": "0.17.12",
    "eslint": "8.36.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.2",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig-paths": "3.11.0",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.3"
  },
  "lint-staged": {
    "*.ts{,x}": "yarn lint"
  },
  "eslintConfig": {
    "extends": [
      "@banka/react"
    ]
  },
  "homepage": "https://github.com/jeremybanka/corners#readme",
  "bugs": {
    "url": "https://github.com/jeremybanka/corners/issues"
  }
}
