{
  "name": "corners",
  "version": "0.0.6",
  "private": false,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "serve": "nodemon ./example/dist/server.js",
    "build:server": "node ./example/esbuild.config.server.js",
    "build:client": "node ./example/esbuild.config.client.js",
    "prebuild:example": "rm -rf example/dist && mkdir example/dist && cp example/src/index.html example/dist",
    "build:example": "concurrently \"yarn build:server\" \"yarn build:client\"",
    "dev": "concurrently \"yarn build:example\" \"yarn serve\"",
    "build": "node ./esbuild.config.js",
    "test": "jest --watch --no-coverage",
    "test:cov": "jest --watch",
    "test:once": "jest",
    "lint": "eslint \"{src,test}/**/*.ts{,x}\"",
    "lint:fix": "yarn lint -- --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-typescript": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@banka/eslint-config-react": "^1.0.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "concurrently": "^6.4.0",
    "esbuild": "^0.14.5",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "react": "^18.0.0-rc.0-next-3dc41d8a2-20211223",
    "react-dom": "^18.0.0-rc.0-next-3dc41d8a2-20211223",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.ts{,x}": "yarn lint"
  },
  "eslintConfig": {
    "extends": [
      "@banka/react"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.5"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
